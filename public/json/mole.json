{
    "version": "0.2.2",
    "cmds": [
        {
            "cmd": "clear",
            "desc": "Clears the console.",
            "alias": ["clear"],
            "flags": [],
            "value": null,
            "api": false
        },
        {
            "cmd": "help",
            "desc": "Shows the help menu.",
            "alias": ["help"],
            "flags": [],
            "value": null,
            "api": false
        },
        {
            "cmd": "man",
            "desc": "Shows the manual for a command.",
            "alias": ["man"],
            "flags": [],
            "value": {
                "name": "CMD",
                "type": "string"
            }
        },
        {
            "cmd": "location",
            "desc": "Shows the current location of the given user login.",
            "alias": ["location", "loc"],
            "flags": [],
            "value": {
                "name": "LOGIN",
                "type": "string"
            },
            "api": true
        },
        {
            "cmd": "loginHistory",
            "desc": "Shows the login history of the given user login.",
            "alias": ["loginHistory", "lHist"],
            "flags": [
                {
                    "flag": "-n",
                    "desc": "Amount of locations to show.",
                    "value": {
                        "name": "N",
                        "type": "number"
                    },
                    "optional": true,
                    "repeatable": false
                }
            ],
            "value": {
                "name": "LOGIN",
                "type": "string"
            },
            "default": {
                "amount": 5
            },
            "api": true
        },
        {
            "cmd": "search",
            "desc": "Search utility to search a user",
            "alias": ["search", "s"],
            "flags": [
                {
                    "flag": "-t",
                    "desc": "Search type.",
                    "value": {
                        "name": "TYPE",
                        "type": "stringElement",
                        "elements": ["login", "first_name", "last_name", "id"]
                    },
                    "optional": false,
                    "repeatable": false
                },
                {
                    "flag": "-c",
                    "desc": "Filter by campus.",
                    "value": {
                        "name": "CAMPUS",
                        "type": "stringElement",
                        "elements": "42CAMPUSES"
                    },
                    "optional": true,
                    "repeatable": false
                },
                {
                    "flag": "--pool_month",
                    "desc": "Filter by month.",
                    "value": {
                        "name": "MONTH",
                        "type": "stringElement",
                        "elements": "MONTHS"
                    },
                    "optional": true,
                    "repeatable": false
                },
                {
                    "flag": "--pool_year",
                    "desc": "Filter by year.",
                    "value": {
                        "name": "YEAR",
                        "type": "number"
                    },
                    "optional": true,
                    "repeatable": false
                }
            ],
            "value": {
                "name": "LOGIN",
                "type": "string"
            },
            "api": true
        }
    ],
    "lib": {
        "MONTHS": ["January", "February", "March", "April", "May", "June", "July", "August", "September", "October", "November", "December"],
        "42CAMPUSES": ["MADRID", "CODAM", "MALAGA", "URDULIZ", "BARCELONA", "WOLFSBURG"],
