            "alias": ["loginHistory", "loginhistory", "lHist", "lhist"],
            "flags": [
                {
                    "flag": "-n",
                    "desc": "Amount of locations to show.",
                    "value": {
                        "name": "N",
                        "type": "number"
                    },
                    "optional": true
                }
            ],
            "value": {
                "name": "LOGIN",
                "type": "string"
            },
            "default": {
                "amount": 5
            },
            "api": true
        },
        {
            "cmd": "search",
            "desc": "Search utility to search a user",
            "alias": ["search", "s"],
            "flags": [
                {
                    "flag": "-t",
                    "desc": "Search type.",
                    "value": {
                        "name": "TYPE",
                        "type": "stringElement",
                        "elements": ["login", "first_name", "last_name", "id"]
                    },
                    "optional": false
                }
            ],
            "value": {
                "name": "LOGIN",
                "type": "string"
            },
            "api": true
        }
